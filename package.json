{
  "name": "app",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "test": "node ./node_modules/.bin/mocha --recursive ./helpers/test-hooks.js ./vendor/*/tests/*.test.js ./modules/*/tests/*.test.js",
    "test:watch": "nodemon --watch modules/**/*.js --exec \"npm test\"",
    "start": "node .",
    "stop": "pm2 stop app",
    "restart": "pm2 restart app",
    "nodemon": "nodemon .",
    "lint": "eslint .",
    "precommit": "npm run lint && npm test",
    "watch": "watch 'npm run lint && npm test' -d -u -p /^node_modules/ .",
    "release:major": "release-it major",
    "release:minor": "release-it minor",
    "release:patch": "release-it patch",
    "webdash": "webdash serve",
    "generate:module": "plop module",
    "postinstall": "./scripts/install-dependencies.js"
  },
  "engines": {
    "node": ">=10.15.3"
  },
  "dependencies": {
    "@sendgrid/mail": "^6.3.1",
    "ajv": "^6.10.0",
    "app-module-path": "^2.2.0",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.2",
    "compression": "^1.7.4",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^2.0.3",
    "cookie-parser": "^1.4.4",
    "debug": "^4.1.1",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-session": "^1.14.1",
    "file-stream-rotator": "^0.4.0",
    "generate-password": "^1.4.1",
    "glob": "^7.1.1",
    "helmet": "^3.16.0",
    "i18next": "^15.0.7",
    "i18next-express-middleware": "^1.7.2",
    "i18next-node-fs-backend": "^2.1.1",
    "lodash": "^4.17.10",
    "method-override": "^3.0.0",
    "mongoose": "^5.4.19",
    "morgan": "^1.9.0",
    "nodemailer": "^5.1.1",
    "nunjucks": "^3.2.0",
    "owasp-password-strength-test": "^1.3.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "path-to-regexp": "^3.0.0",
    "twilio": "^3.29.2",
    "validator": "^10.11.0"
  },
  "main": "./app",
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^5.15.2",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "husky": "^1.3.1",
    "mocha": "^6.0.2",
    "nodemon": "^1.18.10",
    "plop": "^2.3.0",
    "pm2": "^3.4.0",
    "release-it": "^10.3.1",
    "should": "^13.1.2",
    "supertest": "^4.0.2",
    "watch": "^1.0.2"
  },
  "husky": {
    "hooks": {
      "post-merge": "npm i",
      "pre-commit": "npm run lint && npm test"
    }
  }
}
